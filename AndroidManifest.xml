<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.android.fonolo"
      android:versionName="1.0.1" android:versionCode="2">
    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:name=".fonolo4android"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    <activity android:name=".home" android:label="@string/search_name"></activity>
<activity android:name=".list" android:label="@string/list_name"></activity>
<activity android:name=".company" android:label="@string/company_name"></activity>
<activity android:name=".help"
	android:label="@string/help_name" android:theme="@android:style/Theme.Dialog"></activity>
<activity android:name=".call" android:label="@string/call_name"></activity>
<activity android:label="@string/message_name" android:name=".message" android:theme="@android:style/Theme.Dialog"></activity>
<activity android:name=".settings" android:label="@string/settings_name"></activity>
<activity android:name=".eula" android:label="@string/eula_name" android:theme="@android:style/Theme.Light"></activity>
</application>
<uses-permission android:name="android.permission.INTERNET"></uses-permission>


</manifest> 